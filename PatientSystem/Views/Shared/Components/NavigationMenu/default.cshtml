@model IEnumerable<string>

<a asp-action="PatientList"
   asp-controller="Home"
   asp-route-martialStatus="">
    home
</a>

@foreach (string maritalStatus in Model )
{
<a asp-action="PatientList"
   asp-controller="Home"
   asp-route-martialStatus="@maritalStatus"
   asp-route-page="1">
    @maritalStatus
</a>
}
